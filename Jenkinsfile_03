pipeline {
    agent any
    triggers {
        pollSCM 'H/10 * * * *'
    }
    stages {
        stage('Build') {
            steps {
            checkout([
                    $class: 'GitSCM', 
                    includedRegions: 'test3', 
                    workspaceUpdater: [$class: 'CheckoutUpdater']
                ])
                sh 'echo 3'
            }
        }
    }
}
